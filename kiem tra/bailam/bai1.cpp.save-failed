#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,m;
		cin>>n>>m;
		int a[n],b[m];
		map<int,int>mp;
		for(int i=0;i<n;i++){
			cin>>a[i];
			mp[a[i]]=1;

		}
		for(int i=0;i<m;i++){
			cin>>b[i];
			if(mp[b[i]]==0) mp[b[i]]=1;

		}
		for(auto x:mp){
			cout<<x.first<<" ";
		}
		cout<<endl;
		for(auto x:mp){
			if(x.second==1) cout<<x.second<<" ";
		}
		cout<<endl;

	}
}
