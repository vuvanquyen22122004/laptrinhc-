#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,x;
		int a[n];
		for(int i=0;i<n;i++){
			cin>>a[i];
		}
		multimap<int,int>mp;
		for(int i=0;i<n;i++){
			int y=abs(x-a[i]);

			mp.insert({y,a[i]};

		}
		for(auto x:mp){
			cout<<x.second()<<" ";
		}
		cout<<endl;
	}
}
